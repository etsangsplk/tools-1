<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Visual Studio">
  <!-- stackoverflow scheme -->
  <style name="Text" foreground="#000000" background="#f2f2f2"/>
  <style name="Extra" foreground="#008B8B" background=""/>
  <style name="Selection" background="#BCBCBC"/>
  <style name="CurrentLine" background="#f2f2f2"/>
  <style name="IndentLine" foreground="#008B8B"/>
  <style name="VisualWhitespace" foreground="#f57900"/>
  <style name="Keyword" foreground="#00008b" bold="false"/>
  <style name="DataType" foreground="#2b91af" /> 
  <style name="Decimal" foreground="#000000"/>
  <style name="BaseN" foreground="#000000"/>
  <style name="Float" foreground="#000000"/>
  <style name="Char" foreground="#a5120b"/>
  <style name="String" foreground="#800000"/>
  <style name="Comment" foreground="#808080"/>
  <style name="Alert" foreground="#ff0000"/>
  <style name="Error" foreground="#ff0000"/>
  <style name="Function" foreground="#2b91af"/>
  <style name="RegionMarker" foreground="#000000"/>
  <style name="Symbol" foreground="#000000"/> 
  <style name="BuiltinFunc" foreground="#2b91af"/>
  <style name="Predeclared" foreground="#3085c6"/>
  <style name="FuncDecl" foreground="#000000"/>
  <style name="Placeholder" foreground="#a5120b"/>
  <style name="ToDo" foreground="#006000" background="#e0e0e0" bold="true"/>
</style-scheme>
